{
    "version": 2,
    "builds": [
        {
            "src": "dist/Limpia-cristales/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ]
}